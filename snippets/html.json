{
	// Basic tags
	"img": {
		"prefix": "img",
		"body": [
			"<img src=\"$1\" ${2:alt=\"$3\" }/>",
			"$0"
		]
	},
	"a": {
		"prefix": "a",
		"body": [
			"<a href=\"$1\"${2: class=\"$3\"}${4: target=\"${5:_blank}\"}>$6</a>$0",
		]
	},
	"div": {
		"prefix": "div",
		"body": [
			"<div class=\"$1\">",
			"  $0",
			"</div>"
		]
	},
}
