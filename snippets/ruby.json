{
	// Comment headers
	"Main Comment Header": {
		"prefix": "##",
		"body": [
			"#####################################",
			"# $1"
			"#####################################",
			"$0"
		]
	},
	"Sub Comment Header": {
		"prefix": "#-",
		"body": [
			"##### $1 #####",
			"$0",
			"######${1/./#/g}######"
		]
	},
}
